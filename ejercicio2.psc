Algoritmo numero_distinto_de0
	//definir variables
	Definir num,cont Como Entero
	cont<-0;
	Escribir "Ingrese un numero";
	leer num;
	//Proceso 
	Mientras num <> 0 Hacer
		Escribir"Numero leido",num;
		cont<-cont+1
		Escribir "Ingrese un numero";
		leer num;
	FinMientras
	Escribir "Los numeros distinto de 0 son",cont
	
	
FinAlgoritmo

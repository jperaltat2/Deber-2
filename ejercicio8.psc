Algoritmo media
	Definir a,b,c,d,producto,suma como entero
	Definir mediaaritmetica como real;
	Escribir "Ingrese a"
	Leer a
	Escribir "Ingrese b"
	leer b
	Escribir "ingrese c"
	leer c
	Escribir "Ingrese d"
	leer d
	producto <- a*b*c*d
	suma <- a+b+c+d
	mediaaritmetica <- suma/4
	Escribir "El resultado del producto es:",producto
	Escribir " El resultado de la suma es:", suma
	Escribir " El resultado de la mediaaritmetica es:",mediaaritmetica
	
FinAlgoritmo
